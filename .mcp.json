{
    "mcpServers": {
        "fetch": {
            "command": "docker",
            "args": [
                "run",
                "-i",
                "--rm",
                "mcp/fetch"
            ],
            "disabled": false,
            "autoApprove": [
                "fetch",
                "fetch"
            ]
        },
        "openapi-schema": {
            "command": "docker",
            "args": [
                "run",
                "-i",
                "--rm",
                "mcp/openapi-schema:latest"
            ],
            "disabled": false,
            "autoApprove": []
        },
        "notionMCP": {
            "command": "npx",
            "args": [
                "-y",
                "mcp-remote",
                "https://mcp.notion.com/mcp"
            ],
            "disabled": false,
            "autoApprove": []
        },
        "time-mcp": {
            "command": "npx",
            "args": [
                "-y",
                "@smithery/cli@latest",
                "run",
                "@yokingma/time-mcp",
                "--key",
                "d2df686e-54b5-403c-b75e-0531a066d8f3",
                "--profile",
                "elated-silkworm-vhRBZq"
            ],
            "disabled": false,
            "autoApprove": [
                "current_time"
            ]
        },
        "exa": {
            "command": "npx",
            "args": [
                "-y",
                "mcp-remote",
                "https://mcp.exa.ai/mcp?exaApiKey=${EXA_API_KEY}"
            ],
            "env": {
                "EXA_API_KEY": "${EXA_API_KEY}"
            }
        },
        "context7": {
            "command": "npx",
            "args": [
                "-y",
                "@upstash/context7-mcp"
            ],
            "env": {},
            "disabled": false,
            "autoApprove": []
        },
        "google-search": {
            "command": "docker",
            "args": [
                "run",
                "-i",
                "--rm",
                "-e",
                "GOOGLE_API_KEY=${GOOGLE_API_KEY}",
                "-e",
                "GOOGLE_SEARCH_ENGINE_ID=${GOOGLE_SEARCH_ENGINE_ID}",
                "mcp/google-custom-search:latest",
                "stdio"
            ],
            "disabled": false,
            "autoApprove": [
                "search",
                "read_webpage"
            ]
        },
        "google-slides-mcp": {
            "command": "node",
            "args": [
                "/Users/taeyeonkwon/mcp/google-slides-mcp/build/index.js"
            ],
            "env": {
                "GOOGLE_CLIENT_ID": "${GOOGLE_CLIENT_ID}",
                "GOOGLE_CLIENT_SECRET": "${GOOGLE_CLIENT_SECRET}",
                "GOOGLE_REFRESH_TOKEN": "${GOOGLE_REFRESH_TOKEN}"
            },
            "disabled": false,
            "autoApprove": [
                "create_presentation"
            ]
        }
    }
}