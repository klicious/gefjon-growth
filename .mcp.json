{
    "mcpServers": {
        "fetch": {
            "command": "docker",
            "args": [
                "run",
                "-i",
                "--rm",
                "mcp/fetch"
            ],
            "disabled": false,
            "autoApprove": [
                "fetch",
                "fetch"
            ]
        },
        "openapi-schema": {
            "command": "docker",
            "args": [
                "run",
                "-i",
                "--rm",
                "mcp/openapi-schema:latest"
            ],
            "disabled": false,
            "autoApprove": []
        },
        "notionMCP": {
            "command": "npx",
            "args": [
                "-y",
                "mcp-remote",
                "https://mcp.notion.com/mcp"
            ],
            "disabled": false,
            "autoApprove": []
        },
        "time-mcp": {
            "command": "npx",
            "args": [
                "-y",
                "@smithery/cli@latest",
                "run",
                "@yokingma/time-mcp",
                "--key",
                "d2df686e-54b5-403c-b75e-0531a066d8f3",
                "--profile",
                "elated-silkworm-vhRBZq"
            ],
            "disabled": false,
            "autoApprove": [
                "current_time"
            ]
        },
        "exa": {
            "command": "npx",
            "args": [
                "-y",
                "mcp-remote@latest",
                "https://mcp.exa.ai/mcp?exaApiKey=ab6abe6a-035b-4c55-9bd7-ded73eefaada"
            ],
            "disabled": false,
            "autoApprove": [
                "search",
                "get_contents"
            ],
            "env": {
                "NODE_OPTIONS": "--max-old-space-size=4096",
                "FASTMCP_LOG_LEVEL": "ERROR"
            }
        },
        "Context7": {
            "command": "npx",
            "args": [
                "-y",
                "@upstash/context7-mcp"
            ],
            "env": {},
            "disabled": false,
            "autoApprove": []
        },
        "google-search": {
            "command": "docker",
            "args": [
                "run",
                "-i",
                "--rm",
                "-e",
                "GOOGLE_API_KEY=AIzaSyCLxDSYhPeEFQFDxceaToDeUi9wnp76MOs",
                "-e",
                "GOOGLE_SEARCH_ENGINE_ID=0375804ee80fd4e72",
                "mcp/google-custom-search:latest",
                "stdio"
            ],
            "disabled": false,
            "autoApprove": [
                "search",
                "read_webpage"
            ]
        }
    }
}